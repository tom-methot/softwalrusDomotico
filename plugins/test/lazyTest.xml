    <example>Je suis fatiguée que veux tu ?</example>
    <tag>out.action=new Object(); </tag>
    <item>Sarah</item> 
    <one-of>
      <item>active le test 2 <tag>out.action.test="L1";</tag></item>
      <item>fait le test 3   <tag>out.action.test="L2";</tag></item>
    </one-of>
    <tag>out.action._attributes.uri="http://127.0.0.1:8080/sarah/test";</tag>
  </rule>
</grammar>